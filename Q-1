//Question-1
#include<stdio.h>

void arry(){

int v,r;
int sum=0;

	printf("Write array size :- ");
	scanf("%d",&v);

	int o[v];

	printf("\n");	
	printf("Enter array elaments :- \n");	
	for(r=0;r<=v;r++){

  printf("o[%d] = ",r);
  scanf("%d",&o[r]);

  sum += o[r];	}

	printf("\n");	
	printf("Sum of array is %d",sum);}

void main()

{
     array();	

}
